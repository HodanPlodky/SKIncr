(module Main imports (StdLib))

(vars (
  (let X 23)
  (let Y 42)
  (let Z (+ X Y))
))

(functions (

  # this function will NO LONGER cause a runtime exception
  # we now have mutable local variables as well as fn parameters
  (function fail (a b)
    (begin
      (set! a 23)
      a  
    )
  )

  (function main ()
    (begin
      (let seven 7)
      (fail X Y)
      Z
    )
  )

))
