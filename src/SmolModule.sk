value class Module(modName: String, imports: List<Import>, mutVars: List<VarDecl>, funcs: List<FunDecl>) uses Hashable, Orderable

value class Import(moduleName: String) uses Hashable, Orderable

value class VarDecl(varName: String, init: Expr) uses Hashable, Orderable

value class FunDecl(funName: String, params: List<EVar>, body: Expr) uses Hashable, Orderable
